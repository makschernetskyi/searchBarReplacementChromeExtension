{
	"manifest_version": 2,
	"name": "urlReplacementSearch",
	"version": "0.1",
	"description": "extension to search when searchbar is not present",
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"browser_action":{
		"default_icon": "./assets/favicon/search.png",
		"default_popup": "./popup/popup.html"
	}
}